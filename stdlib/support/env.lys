#[extern "env" "putchar"] fun putchar(char: u32): void = panic()

#[extern "env" "printf"] fun printf(number: u32, extra: i32): void = panic()
#[extern "env" "printf"] fun printf(number: u32, extra: u32): void = panic()
#[extern "env" "printf"] fun printf(number: u32, extra: f32): void = panic()
#[extern "env" "printf"] fun printf(number: u32, extra: f64): void = panic()
#[extern "env" "printf"] fun printf(number: u32, extra: u16): void = panic()
#[extern "env" "printf"] fun printf(number: u32, extra: i16): void = panic()
#[extern "env" "printf"] fun printf(number: u32, extra: u8): void = panic()

fun printf(str: bytes, extra: i32): void = printf(str.ptr - 4 as u32, extra)
fun printf(str: bytes, extra: u32): void = printf(str.ptr - 4 as u32, extra)
fun printf(str: bytes, extra: f32): void = printf(str.ptr - 4 as u32, extra)
fun printf(str: bytes, extra: f64): void = printf(str.ptr - 4 as u32, extra)
fun printf(str: bytes, extra: i16): void = printf(str.ptr - 4 as u32, extra)
fun printf(str: bytes, extra: u16): void = printf(str.ptr - 4 as u32, extra)
fun printf(str: bytes, extra: u8): void = printf(str.ptr - 4 as u32, extra)
fun printf(str: bytes): void = printf(str, 0)