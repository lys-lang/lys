type color {
  Red
  Green
  Blue
}


fun test(x: color): color =
  x match {
    // there MUST be an error here ^^^^


    case is Red -> Red
    case is Green -> Green
  }

fun test1(x: color): color =
  x match {
    // there MUST NOT be error here ^^^^


    case is Red -> Red
