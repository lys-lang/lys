type i32
type boolean

type Maybe {
  None
  Some(value: i32)
}

fun test(a: i32): boolean =
  a match {
    case 1 -> true
    else -> false
  }

fun test2(a: i32): i32 =
  a match {
    case 10 -> 1
    case 20 -> 2
    case 30 -> 3
    case 40 -> 4
    case 50 -> 5
    case 60 -> 6
    case 70 -> 7
    case 80 -> 8
    case 90 -> 9
    else -> 0
  }

fun test3(a: Maybe): boolean =
  a match {
    case is Some -> true
    case is None -> false
  }


fun test4(a: Maybe): Some =
  a match {
    case x is Some -> x
    case is None -> Some(0)
  }

