type Real = ???

ns Real {
  fun discriminant(): u64 = 
    %wasm {
      (i64.const 0x400000000)
    }
  
  fun sizeOf(): i32 = 
    8
  
  fun allocationSize(): u32 = 
    ref.allocationSize()
  
  fun apply(number: f32): Real = {
    var ref = fromPointer(system::memory::malloc(sizeOf()))
    number_set(ref, number)
    ref
  }
  
  fun fromPointer(ptr: (i32 | u32)): Real = 
    %wasm {
      (i64.or (i64.const 0x400000000)  (i64.extend_u/i32 (get_local $ptr)))
    }
  
  fun number_get(target: Real): f32 = 
    f32.load(target, 0)
  
  fun number_set(target: Real, value: f32): void = 
    f32.store(target, value, 0)
  
  fun is(a: Real): boolean = 
    %wasm {
      (i64.eq (i64.and (i64.const 0xffffffff00000000)  (get_local $a))  (i64.const 0x400000000))
    }
  
  fun store(lhs: ref, rhs: Real, offset: i32): void = 
    %wasm {
      (i64.store (i32.add (get_local $offset)  (call addressFromRef  (get_local $lhs)))  (get_local $rhs))
    }
  
  fun load(lhs: ref, offset: i32): Real = 
    %wasm {
      (i64.load (i32.add (get_local $offset)  (call addressFromRef  (get_local $lhs))))
    }
}

