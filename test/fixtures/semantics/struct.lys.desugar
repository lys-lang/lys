fun isComplex(number: Number): boolean =
  match number {
    case is Real -> {
      false
    }
    case is Natural -> {
      false
    }
    case is Complex -> {
      val imaginary = $.imaginary
      imaginary != 0.0
    }
    else -> false
  }

fun main(): void = {
  isComplex(Infinity)
  isComplex(Real(0.0))
  isComplex(Natural(123))
  isComplex(Complex(0.0, 99.1))
}